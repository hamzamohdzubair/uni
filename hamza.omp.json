
{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "osc99": true,
  "version": 2,
  "final_space": true,
  "palette": {
    "black": "#262B44",
    "blue": "#4B95E9",
    "green": "#59C9A5",
    "orange": "#F07623",
    "red": "#D81E5B",
    "white": "#E0DEF4",
    "yellow": "#F3AE35"
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:black",
    "template": "<p:yellow,transparent>\ue0b6</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> "
  },
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "background": "#ef5350",
          "foreground": "#FFFB38",
          "style": "diamond",
          "template": "<parentBackground></>  ",
          "type": "root"
        },
        {
          "background": "#FF9248",
          "foreground": "#2d3436",
          "trailing_diamond": "",
          "style": "diamond",
          "template": " {{ .Path }} ",
          "type": "path",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "",
            "style": "agnoster_full"
          }
        },
        {
          "background": "p:green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ],
          "foreground": "p:black",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}"
          ],
          "powerline_symbol": "",
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\uf7a3"
          },
          "style": "powerline",
          "trailing_diamond": "",
          "leading_diamond": "<Background></>",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": " ",
          "type": "node",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a></> ",
            "yarn_icon": " <#348cba></>"
          }
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": " ",
          "type": "python"
        },
        {
          "background": "#f3f0ec",
          "foreground": "#925837",
          "style": "diamond",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "type": "rust"
        },
        {
          "background": "#b2bec3",
          "foreground": "#222222",
          "leading_diamond": "",
          "trailing_diamond": "",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os",
          "properties": {
            "linux": "",
            "macos": "",
            "windows": ""
          }
        },
        {
          "type": "wakatime",
          "style": "diamond",
          "leading_diamond":  "",
          "trailing_diamond": "",
          "invert_powerline": true,
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#007acc",
          "properties": {
              "url": "https://wakatime.com/api/v1/users/current/summaries?start=today&end=today&api_key=9e410d09-10e9-49d1-9f16-de35b13ebe8b",
              "cache_timeout": 20,
              "http_timeout": 2000
          }
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "foreground": "#262626",
          "invert_powerline": true,
          "style": "diamond",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} ",
          "type": "battery",
          "leading_diamond":  "",
          "trailing_diamond": "",
          "properties": {
            "charged_icon": " ",
            "charging_icon": " ",
            "discharging_icon": " "
          }
        },
        {
          "background": "#40c4ff",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "leading_diamond": "",
          "style": "diamond",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "type": "time",
          "properties": {
            "time_format": "Mon _2 Jan"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#c386f1",
          "template": " {{ if .SSHSession }} {{ end }}{{ .UserName }}"
        },
        {
          "background": "#40c4ff",
          "foreground": "#e0f8ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "style": "powerline",
          "powerline_symbol": "",
          "type": "exit",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "alignment": "right",
      "type": "rprompt",
      "segments": [
        {
          "background": "#c386f1",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "leading_diamond": "",
          "style": "accordion",
          "template": "{{ .CurrentDate | date .Format }}",
          "trailing_diamond": "",
          "type": "time",
          "interactive": true,
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": " ",
          "powerline_symbol": "",
          "template": " 羽{{ .FormattedMs }} ",
          "type": "executiontime",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        }
      ]
    }
  ]
}