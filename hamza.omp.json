{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "osc99": true,
  "version": 2,
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "background": "#ef5350",
          "foreground": "#FFFB38",
          "style": "diamond",
          "template": "<parentBackground></>  ",
          "type": "root"
        },
        {
          "background": "#FF9248",
          "foreground": "#2d3436",
          "leading_diamond": "",
          "trailing_diamond": "",
          "style": "diamond",
          "template": "{{ .Path }} ",
          "type": "path",
          "properties": {
            "folder_icon": "",
            "home_icon": "",
            "style": "full"
          }
        },
        {
          "background": "#FFFB38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "foreground": "#011627",
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350>  {{ .Staging.String }}</>{{ end }} ",
          "type": "git",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "",
          "template": " 羽{{ .FormattedMs }}",
          "type": "executiontime",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": " ",
          "type": "node",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a></> ",
            "yarn_icon": " <#348cba></>"
          }
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": " ",
          "type": "python"
        },
        {
          "background": "#f3f0ec",
          "foreground": "#925837",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": " ",
          "type": "rust"
        },
        {
          "background": "#b2bec3",
          "foreground": "#222222",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}<#262626> </>",
          "type": "os",
          "properties": {
            "linux": "",
            "macos": "",
            "windows": ""
          }
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "foreground": "#262626",
          "invert_powerline": true,
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} <#262626></>",
          "type": "battery",
          "properties": {
            "charged_icon": " ",
            "charging_icon": " ",
            "discharging_icon": " "
          }
        },
        {
          "background": "#40c4ff",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "leading_diamond": "",
          "style": "diamond",
          "template": "  {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "",
          "type": "time",
          "properties": {
            "time_format": "Mon _2 Jan"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#c386f1",
          "leading_diamond": "",
          "template": "{{ if .SSHSession }} {{ end }}{{ .UserName }}"
        },
        {
          "background": "#40c4ff",
          "foreground": "#e0f8ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "style": "powerline",
          "powerline_symbol": "",
          "type": "exit",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "background": "#c386f1",
          "foreground": "#ffffff",
          "powerline_symbol": " ",
          "leading_diamond": "",
          "style": "powerline",
          "template": "{{ .CurrentDate | date .Format }}",
          "trailing_diamond": "",
          "type": "time",
          "properties": {
            "time_format": "1504"
          }
        }
      ]
    }
  ]
}